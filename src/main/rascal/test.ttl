test OkInteger1 [[
    function testCond() do Int x =2 end testCond 
]]

test OkInteger2 [[
    function testCond() do Int y=4 Int x=y end testCond 
]]

test NoOkInteger1 [[
    function testCond() do Int y=4 Int x=z end testCond 
]] expect {"Undefined variable ⁠ z ⁠"}

test NoOkInteger2 [[
    function testCond() do String y="HolaMundo" Int x=y end testCond 
]] expect {"Incorrect initialization, expected ⁠ int ⁠, found ⁠ str ⁠"}

test OkFloat1 [[
    function testCond() do Float x =2.5 end testCond 
]]

test OkFloat2 [[
    function testCond() do Float y=4.2 Float x=y end testCond 
]]

test NoOkFloat1 [[
    function testCond() do Float y=4.3 Int x=z end testCond 
]] expect {"Undefined variable ⁠ z ⁠"}